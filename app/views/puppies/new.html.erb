<h1>Puppies#new</h1>

<% if @puppy.errors.any? %>
  <% @puppy.errors.full_messages.each do |error|  %>
  <p>
  <%= error %>
  </p>
  <% end %>
<% end %>

<%= form_for @puppy, url:"/puppies" do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :breed %>
  <%= f.text_field :breed %>
  <%= f.submit %>
<% end %>
<p>
  <%= link_to"Back", :back %>
</p>

<%= image_tag("p5.jpg") %>
